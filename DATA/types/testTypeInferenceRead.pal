program inferenceRead;

function f() : bool
    return read();

procedure main()
var n : int;
    c : char;
    b : bool;
begin
    c := 'a';
    n := 2 + read();

    write("n = ");
    write(n);
    write("\n");

    b := chr(read()) = read();

    write("b = ");
    write(b);
    write("\n");

    c := read();

    write("c = ");
    write(c);
    write("\n");

    b := f();

    write("b = ");
    write(b);
    write("\n");
end;
